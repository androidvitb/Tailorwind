{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/tools-and-generator/?$",
      "dest": "/src/pages/tools-and-generator.html"
    },
    {
      "src": "^/button-generator/?$",
      "dest": "/src/pages/button-generator.html"
    },
    {
      "src": "^/card-generator/?$",
      "dest": "/src/pages/card-generator.html"
    },
    {
      "src": "^/grid-layout-generator/?$",
      "dest": "/src/pages/grid-layout-generator.html"
    },
    {
      "src": "^/table-generator/?$",
      "dest": "/src/pages/table-generator.html"
    },
    {
      "src": "^/form-generator/?$",
      "dest": "/src/pages/form-generator.html"
    },
    {
      "src": "^/navbar-generator/?$",
      "dest": "/src/pages/navbar-generator.html"
    },
    {
      "src": "^/color-palette/?$",
      "dest": "/src/pages/color-palette.html"
    },
    {
      "src": "^/get-started/?$",
      "dest": "/src/pages/get-started.html"
    },
    {
      "src": "^/sign-up/?$",
      "dest": "/src/pages/sign-up.html"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/src/assets/$1"
    },
    {
      "src": "^/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}